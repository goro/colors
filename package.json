{
  "name": "colors",
  "version": "1.0.0",
  "description": "Palette building tool",
  "main": "index.js",
  "scripts": {
    "test": "mocha './specs/**/*.spec.js' --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "serve": "webpack-dev-server --progress --colors --content-base public/",
    "dev": "parallelshell 'npm run test:watch' 'npm run serve'",
    "build": "set NODE_ENV=production && webpack --config webpack.config.min.js --progress --colors"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "expect": "^1.16.0",
    "expect-jsx": "^2.4.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "react-addons-test-utils": "^0.14.8",
    "redux-devtools": "^3.2.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1"
  }
}
